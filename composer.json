{
    "name": "rnewton/twiggy",
    "type": "library",
    "description": "Twiggy is a simple database migration tool for PHP 5.4+",
    "keywords": [
        "Database",
        "Migration"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Newton",
            "email": "robert.newton11@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/console": "~2.3",
        "symfony/filesystem": "~2.3",
        "nette/database": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },
    "autoload": {
        "psr-0": {
            "Twiggy": "src/"
        }
    },
    "bin": [
        "bin/twiggy"
    ]
}